{
    "name": "agatanga/flags",
    "description": "Country flags in SVG format for Laravel application.",
    "keywords": ["Blade", "Flags", "Laravel", "SVG"],
    "homepage": "https://github.com/agatanga/flags",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/agatanga/flags/issues",
        "source": "https://github.com/agatanga/flags"
    },
    "authors": [
        {
            "name": "Vova Yatsyuk",
            "email": "vova.yatsyuk@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "components/flag-icon-css": "^3.0.0",
        "illuminate/contracts": "^7.14|^8.0",
        "illuminate/filesystem": "^7.14|^8.0",
        "illuminate/support": "^7.14|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Agatanga\\Flags\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Agatanga\\Flags\\FlagsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
